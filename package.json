{
  "name": "xpe-agent",
  "version": "0.0.1",
  "description": "Autonomous AI Agent for XPE Panel - Unrestricted System Access",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "install": "npm install",
    "push": "git add . && git commit -m \"Update by xpe-agent\" && git push origin main",
    "status": "node -e \"console.log('XPE Agent Status: Ready');\""
  },
  "keywords": [
    "ai",
    "agent",
    "autonomous",
    "devops",
    "ollama",
    "xpe"
  ],
  "author": "xpe-hub",
  "license": "MIT",
  "dependencies": {
    "ai": "^4.0.0",
    "@ai-sdk/groq": "^0.0.1",
    "ollama-ai-provider": "^0.1.0",
    "zod": "^3.23.8",
    "simple-git": "^3.27.0",
    "octokit": "^4.0.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "tsx": "^4.19.0",
    "@types/node": "^20.14.0",
    "@types/fs-extra": "^11.0.4"
  }
}
